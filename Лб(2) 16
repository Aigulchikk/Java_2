public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Введите количество шариков мороженого: ");
        int k = scanner.nextInt();
        if ((k % 3 == 0) || (k % 5 == 0)){
            System.out.print("Yes");
        }
        else {
            System.out.print("No");
        }
    }
}
